<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Data</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr" crossorigin="anonymous">
</head>

<body>
    <div class="container">
        <div class="row justify-content-center mt-5">
            <div class="col-md-8 col-sm-12">
                <h1 class="text-center">Assigned Batch  Data</h1>
                <table class="table table-bordered border-primary table-hover table-striped text-center">
                    <thead>
                        <tr class="table-dark">
                            <th>#</th>
                            <th>Student ID</th>
                            <th>Student Name</th>
                            <th>Batch ID</th>
                            <th>Batch Name</th>
                            <th>Course ID</th>
                            <th>Course</th>


                        </tr>
                    </thead>
                    <tbody>
                        <% let count=1; student_data.forEach(student=> { %>
                            <tr>
                                <th>
                                    <%= count %>
                                </th>
                                <td>
                                    <%= student.student_id %>
                                </td>
                                <td>
                                    <%= student.student_name %>
                                </td>
                                
                                <td>
                                    <%= student.batch_id===null?"not assgined":student.batch_id %>
                                </td>

                                <td>
                                    <%= student.batch_name===null?"not assgined":student.batch_name %>
                                </td>

                                 <td>
                                    <%= student.course_id===null?"not assgined":student.course_id %>
                                </td>
                                <td>
                                    <%= student.course_name===null?"undefined":student.course_name %>
                                </td>

                            </tr>
                            <% count++; }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>

</html>