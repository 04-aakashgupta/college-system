<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Data</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr" crossorigin="anonymous">
</head>

<body>
    <div class="container">
        <div class="row justify-content-center mt-5">
            <div class="col-md-8 col-sm-12">
                <h1 class="text-center">Batch Data</h1>
                <table class="table table-bordered border-primary table-hover table-striped text-center">
                    <thead>
                        <tr class="table-dark">
                            <th>#</th>
                            <th>ID</th>
                            <th>Batch Name</th>
                            <th>Course Name</th>

                             <th>Start Date</th>
                            <th>End Date</th>
                   
                        </tr>
                    </thead>
                    <tbody>
                        <% let count=1; batch_data.forEach(batch=> { %>
                            <tr>
                                <th>
                                    <%= count %>
                                </th>
                                <td>
                                    <%= batch.batch_id %>
                                </td>
                                <td>
                                    <%= batch.batch_name %>
                                </td>

                                 <td>
                                    <%= batch.course_name %>
                                </td>

                                  <td>
                                    <%= batch.batch_start_date.toLocaleDateString() %>
                                </td>  <td>
                                    <%= batch.batch_end_date.toLocaleDateString() %>
                                </td>
                                
                            </tr>
                            <% count++; });  %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</body>

</html>